{
	"name": "Mindframe Theme",
	"type": "dark",
	"colors": {
	  "editor.background": "#1e1e1e",
	  "editor.foreground": "#d4d4d4",
	  "editorCursor.foreground": "#ffffff",
	  "editor.lineHighlightBackground": "#2c2c2c",
	  "editor.selectionBackground": "#3e3e3e",
	  "editor.selectionHighlightBackground": "#3a3a3a",
	  "editorIndentGuide.background": "#555555"
	},
	"tokenColors": [
	  {
		"scope": [
		  "entity.name.tag.xml.example",
		  "entity.name.tag.xml.theory",
		  "entity.name.tag.xml.guidance",
		  "entity.name.tag.xml.state",
		  "entity.name.tag.xml.transcript",
		  "entity.name.tag.xml.notes"
		],
		"settings": {
		  "foreground": "#c586c0",
		  "fontStyle": "italic"
		}
	  },
	  {
		"scope": "markup.bold.markdown",
		"settings": {
		  "foreground": "#dcdcaa",
		  "fontStyle": "bold"
		}
	  },
	  {
		"scope": "markup.italic.markdown",
		"settings": {
		  "foreground": "#ce9178",
		  "fontStyle": "italic"
		}
	  },
	  {
		"scope": "markup.code.markdown",
		"settings": {
		  "foreground": "#d16969"
		}
	  },
	  {
		"scope": "markup.heading.markdown",
		"settings": {
		  "foreground": "#9cdcfe",
		  "fontStyle": "bold"
		}
	  },
	  {
		"scope": "markup.ordered-list.markdown",
		"settings": {
		  "foreground": "#9cdcfe"
		}
	  },
	  {
		"scope": "markup.unordered-list.markdown",
		"settings": {
		  "foreground": "#9cdcfe"
		}
	  },
	  {
		"scope": "text.html.basic",
		"settings": {
		  "foreground": "#d7afaf"
		}
	  },
	  {
		"scope": "keyword.other.django-template",
		"settings": {
		  "foreground": "#569cd6"
		}
	  }
	]
  }
